{
    "name": "I Don't Care About AngularJS",
    "description" : "Adds '-AngularJS' to every google.at query",
    "version": "1.0",
    "permissions": [
        "storage",
        "webRequest",
        "webRequestBlocking",
        "*://*.google.at/"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "content_scripts": [
        {
            "matches": ["https://www.google.at/*"],
            "js": ["content.js"],
            "css": ["content.css"],
            "run_at": "document_start"
        }
    ],
    "manifest_version": 2
}